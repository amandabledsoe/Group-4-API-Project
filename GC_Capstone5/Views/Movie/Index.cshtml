
@using GC_Capstone5.Models

@model SearchRootobject
@if (Model != null && Model.results != null && Model.results.Length > 0)
{
    <table>
        @for (int i = 0; i < Model.results.Length; i++)
        {
        <tr>
            <td><p>@Model.results[i].title</p>
                @(DateTime.Parse(Model.results[i].release_date).ToString("yyyy-MM-dd"))</td>
            <td><img src="@Model.results[i].poster_path" /></td>
        </tr>
        }
    </table>
}

